{
  "schemaVersion": 38,
  "title": "Lynx HTTP Service Dashboard",
  "editable": true,
  "timezone": "browser",
  "version": 3,
  "tags": ["lynx", "http", "microservices", "monitoring"],
  "style": "dark",
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": [
      {
        "name": "DS_PROM",
        "type": "datasource",
        "query": "prometheus",
        "current": {"text": "Prometheus", "value": "Prometheus"},
        "hide": 2
      },
      {
        "name": "rate_interval",
        "type": "custom",
        "query": "1m,5m,15m",
        "current": {"text": "5m", "value": "5m"},
        "hide": 0
      },
      {
        "name": "job",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "$DS_PROM"},
        "query": "label_values(up, job)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "hide": 0
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "$DS_PROM"},
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "hide": 0
      },
      {
        "name": "url",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "$DS_PROM"},
        "query": "label_values(lynx_http_requests_total, path)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Requests/sec",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "$DS_PROM"},
      "targets": [
        {
          "expr": "sum(rate(lynx_http_requests_total{job=~\"$job\",instance=~\"$instance\"}[$rate_interval]))",
          "legendFormat": "Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "displayMode": "gradient-gauge",
            "gradientMode": "hue",
            "orientation": "auto",
            "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
            "textMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 500}
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Response Time (P95)",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "$DS_PROM"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(lynx_http_request_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))",
          "legendFormat": "P95 Latency"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "displayMode": "gradient-gauge",
            "gradientMode": "hue",
            "orientation": "auto",
            "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
            "textMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.1},
              {"color": "red", "value": 0.5}
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Error Rate",
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "$DS_PROM"},
      "targets": [
        {
          "expr": "sum(rate(lynx_http_errors_total{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])) / sum(rate(lynx_http_requests_total{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])) * 100",
